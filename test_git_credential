pipeline {
    agent any

    environment {
        DOCKER_IMAGE = 'sidharthsingh7/ss_backend'
        DOCKER_TAG = "0.0.1.RELEASE"
    }

    stages {
        stage('Checkout') {
            steps {
                git scm
            }
        }

        stage('Validate') {
            steps {
                script {
                    sh 'ls -la'
                }
            }
        }
